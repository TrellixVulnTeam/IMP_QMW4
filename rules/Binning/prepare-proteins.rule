rule binning_prepare_proteins:
    #IN: <directory w/ unprepared .faa files> <OUT_DIR>
    input:
       "Binning/separate_bins.done"
    output:
       "Binning/prepare_proteins.done"
    shell:
        """
        {SRCDIR}/prepareProteins.sh Binning/clusterFiles Binning/clusterFiles/preparedproteins
        touch {output}
        """

